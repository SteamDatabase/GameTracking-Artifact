<root>
  <styles>
    <include src="s2r://panorama/styles/dota_card_game_styles.vcss_c" />
    <include src="s2r://panorama/styles/tooltips/tooltip_inspect_mana.vcss_c" />
  </styles>

  <DCGTooltipInspectMana class="InspectManaContents" hittestchildren="true" hittest="true">
    <Label class="InspectManaTitle" text="#DCG_Glossary_Inspect_Mana_Title" html="true"/>
    <Label class="InspectManaText" text="#DCG_Glossary_Inspect_Mana" html="true"/>
    <Panel class="CrossLane">
      <Panel class="CrossLaneTitleRegion">
        <Panel class="CrossLaneLeftIcon"/>
        <Panel class="CrossLaneRightIcon"/>
        <Label class="CrossLaneTitle" text="#DCG_Glossary_Inspect_Crosslane_Title" html="true"/>
      </Panel>
      <Panel class="CrossLaneTextPanel">
        <Label class="CrossLaneText" text="#DCG_Glossary_Inspect_Crosslane" html="true"/>
      </Panel>
    </Panel>
    <Panel class="Initiative">
      <Panel class="InitiativeTitleRegion">
        <Panel class="InitiativeIcon"/>
        <Label class="InitiativeTitle" text="#DCG_Glossary_Inspect_Initiative_Title" html="true"/>
      </Panel>
      <Panel class="InitiativePanel">
        <Label class="InitiativeText" text="#DCG_Glossary_Inspect_Initiative" html="true"/>
      </Panel>
    </Panel>
  </DCGTooltipInspectMana>
</root>